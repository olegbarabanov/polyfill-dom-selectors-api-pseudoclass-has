{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "sourceMap": true,
    "noImplicitOverride": true,
    "declaration": true,
    "outDir": "./dist/esm",
    "lib": ["ESNext", "scripthost", "DOM","DOM.Iterable"],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["jest", "node"]
  },
  "exclude": ["node_modules", "dist"]
}